@url = http://localhost:5000/users-module
@email = user@confab.io
@password = secret
@role = user
@accessToken = secret

###
GET {{url}}

### 
GET {{url}}/account Authorization: Bearer {{accessToken}}

###
POST {{url}}/account/sign-up 
content-Type: application/json

{
    "email": "{{email}}",
    "password": "{{password}}",
    "role": "{{role}}",
    "claims": {
        "permissions": ["conferences", "hosts", "speakers", "users"]
    }
}